    <plurals name="schedule_block_subtitle"
             description="Shown in the schedule to indicate how many sessions are available on that particular time slot.">
        <item quantity="one"><xliff:g id="number_of_sessions">%1$d</xliff:g> sesión disponible.</item>
        <item quantity="other"><xliff:g id="number_of_sessions">%1$d</xliff:g> sesiones disponibles.</item>
