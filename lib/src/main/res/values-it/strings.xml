<resources xmlns:tools="http://schemas.android.com/tools" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <plurals name="schedule_block_subtitle"
             description="Shown in the schedule to indicate how many sessions are available on that particular time slot.">
        <item quantity="one"><xliff:g id="number_of_sessions">%1$d</xliff:g> sessione disponibile</item>
        <item quantity="other"><xliff:g id="number_of_sessions">%1$d</xliff:g> sessioni disponibili</item>
    </plurals>

    <plurals name="session_notification_text"
             description="Shows as a notification on the system bar to alert user that some of their sessions are about to begin.">
        <item quantity="one">e <xliff:g id="num_sessions">%2$d</xliff:g> altra tra <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
        <item quantity="other">e altre <xliff:g id="num_sessions">%2$d</xliff:g> tra <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
    </plurals>

    <plurals name="session_notification_ticker"
             description="Shown as a notification on the system bar to alert user that some of their sessions are about to begin.">
        <item quantity="one">%1$d sessione sta per iniziare.</item>
        <item quantity="other">%1$d sessioni stanno per iniziare.</item>
    </plurals>

    <plurals name="session_plurals"
             description="Indicates a certain number of sessions.">
        <item quantity="one">%1$d sessione</item>
        <item quantity="other">%1$d sessioni</item>
    </plurals>

    <!-- Title for the notification that alerts the user that some of their sessions are about to begin. -->
    <plurals name="session_notification_title">
        <item quantity="one"><xliff:g id="num_sessions">%2$d</xliff:g> sessione sta per iniziare tra <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
        <item quantity="other"><xliff:g id="num_sessions">%2$d</xliff:g> sessioni stanno per iniziare tra <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
    </plurals>

