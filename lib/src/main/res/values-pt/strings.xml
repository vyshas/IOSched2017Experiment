<resources xmlns:tools="http://schemas.android.com/tools" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <plurals name="schedule_block_subtitle"
             description="Shown in the schedule to indicate how many sessions are available on that particular time slot.">
        <item quantity="one"><xliff:g id="number_of_sessions">%1$d</xliff:g> sessão disponível</item>
        <item quantity="other"><xliff:g id="number_of_sessions">%1$d</xliff:g> sessões disponíveis</item>
    </plurals>

    <plurals name="session_notification_text"
             description="Shows as a notification on the system bar to alert user that some of their sessions are about to begin.">
        <item quantity="one">e <xliff:g id="num_sessions">%2$d</xliff:g> sessão começando em <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
        <item quantity="other">e <xliff:g id="num_sessions">%2$d</xliff:g> outras começando em <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
    </plurals>

    <plurals name="session_notification_ticker"
             description="Shown as a notification on the system bar to alert user that some of their sessions are about to begin.">
        <item quantity="one">%1$d sessão está prestes a começar.</item>
        <item quantity="other">%1$d sessões estão prestes a começar.</item>
    </plurals>

    <plurals name="session_plurals"
             description="Indicates a certain number of sessions.">
        <item quantity="one">%1$d sessão</item>
        <item quantity="other">%1$d sessões</item>
    </plurals>

    <!-- Title for the notification that alerts the user that some of their sessions are about to begin. -->
